<thlogic>
    <attr sel="#headId"
          th:replace="~{fragments/head :: headFragment}"/>
    <attr sel="#game-title" th:text="#{game.title}"/>

    <attr sel="#main-message"
          th:text="${__${T(learn.util.AttributeNames).MAIN_MESSAGE}__}"/>
    <attr sel="#result-message"
          th:text="${__${T(learn.util.AttributeNames).RESULT_MESSAGE}__}"/>
    <attr sel="#game-form"
          th:action="@{__${T(learn.util.GameMappings).PLAY}__}"/>
    <attr sel="#footerId"
          th:replace="~{fragments/footer :: footerFragment}"/>

</thlogic>